<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordInputBox">
        <attr name="amount" format="integer" />
        <attr name="defaultBg" format="reference" />
        <attr name="inputBg" format="reference" />
        <attr name="boxWidth" format="dimension" />
        <attr name="boxHeight" format="dimension" />
        <attr name="margin" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="inputType" format="enum">
            <enum name="phone" value="1" />
            <enum name="text" value="2" />
            <enum name="number" value="3" />
            <enum name="password" value="4" />
        </attr>
    </declare-styleable>
</resources>